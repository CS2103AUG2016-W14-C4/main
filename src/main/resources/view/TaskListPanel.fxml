<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.taskitty.ui.TaskListPanel">
    <stylesheets>
        <URL value="@DarkTheme.css" />
        <URL value="@Extensions.css" />
    </stylesheets>
    <children>
      <HBox alignment="BOTTOM_LEFT" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="4.0" left="4.0" right="4.0" />
         </VBox.margin>
         <children>
            <ImageView fx:id="listIcon" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
              <Label fx:id="header" styleClass="pane-title" text="\$header">
               <padding>
                  <Insets left="5.0" />
               </padding></Label>
         </children>
      </HBox>
        <ListView fx:id="taskListView" minWidth="330.0" VBox.vgrow="ALWAYS" />
    </children>
</VBox>
